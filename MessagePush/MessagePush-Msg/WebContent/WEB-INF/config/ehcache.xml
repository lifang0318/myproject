<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="100000" eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="false" />

	<cache name="bankCardCache" timeToIdleSeconds="600"
		timeToLiveSeconds="600" maxElementsInMemory="100000" eternal="false"
		overflowToDisk="false" />
	
	<cache name="registerCache" timeToIdleSeconds="600"
		timeToLiveSeconds="600" maxElementsInMemory="100000" eternal="false"
		overflowToDisk="false" />
	
	<cache name="accountCache" timeToIdleSeconds="600"
		timeToLiveSeconds="600" maxElementsInMemory="100000" eternal="false"
		overflowToDisk="false" />

</ehcache>